HTML
 div class container - runt hela
 div class form i container
 div class count runt input
 div class list runt listor
 
 div class="inkomst"  ändra till -- income
 span class="inkomstData" ändra -- incomeData
        
 div class="kostnad"  ändra till -- expense
 span class="kostnadsData" ändra -- expenseData
        
 div class="Resultat" ändra till -- result (används den ens?)
 span class="totalt"  ändra till -- total 

 div class="inomstlista" -- incomeList
 ul class="listaIncome"  -- listIn

 div class="kostnadslista" -- expenseList
 ul class="listaKostnad"  -- listOut

 button class="rensa"  -- clear
 button class="add"

    <select name="" id="selectOption">
        <option value="+">+</option>
        <option value="-">-</option>
    
    input type="text" id="description">
    input type="number" id="value">


         -- JS --
 
    var vinstLista = []; -- resultList
    var kostnadsLista = []; -- expenseList
    var inkomstLista = []; -- incomeList
    var options = document.querySelector("#selectOption")
    var description = document.querySelector("#description").value
    var value = document.querySelector("#value").value
    var inkomstData = document.querySelector(".inkomstData") -- incomeData
    var ul = document.querySelector(".listaIncome")  -- listIn 
    vinstLista.push(Number(value))  -- resultList
    inkomstLista.push(Number(value)) --  incomeList
    var description = document.querySelector("#description").value
    var value = document.querySelector("#value").value
    kostnadsLista.push(Number(-value)) -- expenseList
    var kostnadsData = document.querySelector(".kostnadsData") -- expenseData
    var li = document.createElement("li");
    var ul = document.querySelector(".listaKostnad") -- listOut 
    var  summa = 0 -- amount 
    var kostnad = 0  -- expense
    var countKostnad = document.querySelector(".kostnadsData") -- countExpense
    var inkomst = 0 -- income  -- incomeList
    var countInkomst = document.querySelector(".inkomstData") -- countIncome
    var add = document.querySelector(".add")
    var clean = document.querySelector(".rensa") --clear
    
